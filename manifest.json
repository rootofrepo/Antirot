{
  "manifest_version": 2,
  "name": "Antirot",
  "version": "1.0",
  "description": "https://github.com/rootofrepo/Antirot",
  "permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
